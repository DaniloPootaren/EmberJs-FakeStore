{{page-title "Products"}}
{{outlet}}
<div class="product-container">
  <div class="container">
    <div class="row">
      {{#each this.model as |item|}}
      <div class="col">
        <ProductItem @item={{item}} @handleCartAction={{action
          this.addItem item }} />
      </div>
      {{/each}}
    </div>
  </div>
</div>
<Pagination />
<ShoppingButton />